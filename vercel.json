{
  "framework": "nextjs",
  "buildCommand": "npm run prisma:generate && NODE_OPTIONS='--max-old-space-size=4096' npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm ci",
  "env": {
    "NODE_VERSION": "20"
  },
  "functions": {
    "app/api/auth/[...nextauth]/route.ts": {
      "maxDuration": 30
    }
  }
}
